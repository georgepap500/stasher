{
  "name": "front_react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.1.1",
    "moment": "^2.22.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-scripts": "1.1.4",
    "react-select": "^1.2.1",
    "react-svg": "^4.0.2",
    "reactstrap": "^6.0.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "NODE_PATH=./src && ./build.sh $API && PORT=$PORT && npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css && node build-js",
    "build-css": "node-sass-chokidar src/assets/scss -o src/assets/css",
    "watch-css": "npm run build-css && node-sass-chokidar src/assets/scss -o src/assets/css --watch --recursive",
    "predeploy": "./build.sh $ENV && NODE_PATH=./src && npm-run-all build-css build-js && echo $ENV '~~~~~ BUILT ~~~~~'",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.15.0",
    "jest": "^22.4.4",
    "jest-cli": "^22.4.4",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "redux-mock-store": "^1.5.1"
  }
}
